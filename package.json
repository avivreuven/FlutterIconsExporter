    "license": "MIT",
    "dependencies": {
        "@supernovaio/export-helpers": "latest",
        "@supernovaio/icons": "latest",
        "@supernovaio/sdk": "^2.0.30",
        "@supernovaio/sdk-exporters": "latest"
    },
    "devDependencies": {
        "@types/node": "^22.10.2",
        "prettier": "latest",
        "rimraf": "^6.0.1",
        "svgtofont": "^4.2.3",
        "terser-webpack-plugin": "latest",
        "ts-loader": "latest",
        "typescript": "latest",
        "webpack": "5.97.0",
        "webpack-cli": "^5.1.4"
    }
}
